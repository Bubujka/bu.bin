#!/bin/bash
# Запустить хром через socks5 туннель
. ~/.bashrc

k9 chromium-browser
chromium-browser \
  --disable-background-networking \
  --disable-gpu \
  --proxy-server="socks5://localhost:9999" \
  --proxy-bypass-list="127.0.0.1;192.168.*.*;local.bubujka.org;*.webo;pikabu.ru;yandex.ru" \
  https://ifconfig.io

i3-workspace www
